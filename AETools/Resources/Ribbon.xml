      <tab id="twitterPanel" label="Twitter" command="twitterPanel" >
        <group id="twitterGroup" label="Twitter" layoutOrientation="horizontal" itemSpacing="1">
          <container id="twitterContainer" layoutOrientation="horizontal" isGroup="false" verticalAlign="middle">
            <button id="twitterIsOn" label="Twitter" size="large" command="twitterIsOn" />
            <button id="twitterPost" label="Post Tweet" size="large" command="twitterPost" />
            <button id="twitterPostImage" label="Post Image" size="large" command="twitterPostImage" />
            <button id="twitterPostMovie" label="Post Movie" size="large" command="twitterPostMovie" />
          </container>
        </group>
        <group id="twitterCheckboxesGroup" label="Options" layoutOrientation="horizontal" itemSpacing="3">
          <container id="twitterCheckboxesContainer" layoutOrientation="vertical" isGroup="false" verticalAlign="middle" itemSpacing="3">
            <checkbox id="twitterIsShowingOwnTweets" command="twitterIsShowingOwnTweets" />
            <checkbox id="twitterIsCheckingDirectMessages" command="twitterIsCheckingDirectMessages" />
            <checkbox id="twitterIsAnnoying" command="twitterIsAnnoying" />
          </container>
        </group>
      </tab>
